package com.grant.smith;

import java.util.ArrayList;
import java.util.Iterator;
import java.util.List;

public class Route extends ArrayList{

	private void mergeRoutes(Route route1, Route route2){
		soln.remove(route1);
		soln.remove(route2);
		route1.addAll(route2);
		soln.add(route1);
	}

	private double routeDemand(List<Customer> route){
		double demand = 0.0f;
		Iterator<Customer> it = route.iterator();
		Customer customer;
		while(it.hasNext()){
			customer = it.next();
			demand += customer.c;
		}
		return demand;
	}
	
}
