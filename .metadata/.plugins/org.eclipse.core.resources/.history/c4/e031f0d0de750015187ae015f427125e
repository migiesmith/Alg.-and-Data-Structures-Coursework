package com.grant.smith;

import java.awt.geom.Point2D;


public class SavingsNode extends Point2D.Double{

	// Requirements of the customer (number to be delivered)
	public int saving;
	public SavingsNode(Customer x, Customer y, int saving){
		this.x = x;
		this.y = y;
		this.saving = saving;
	}
}
